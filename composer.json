{
    "name": "bssd-ltd/eloquent-repository",
    "description": "Eloquent Repository for Lumen",
    "keywords": [
        "laravel-repository",
        "eloquent-repository",
        "eloquent-repository-generator",
        "repository",
        "repository-generator",
        "eloquent",
        "model"
    ],
    "homepage": "https://github.com/bssd-ltd/eloquent-repository",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Orkhan Ahmadov",
            "email": "ahmadov90@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Tuyen",
            "email": "tuyen@bssd.vn",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "haruncpi/laravel-id-generator": "^1.0",
        "illuminate/console": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/support": "^8.0",
        "laravel/lumen-framework": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Bssd\\EloquentRepository\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bssd\\EloquentRepository\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bssd\\EloquentRepository\\ServiceProvider"
            ]
        }
    }
}
